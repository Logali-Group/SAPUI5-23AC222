<core:FragmentDefinition
        xmlns="sap.m"
        xmlns:core="sap.ui.core">

        <Dialog>    

            <content>

                <Table items="{jsonEmployees>Orders}">
                    <columns>
                        <Column>
                            <Label text="{i18n>orderID}"/>
                        </Column>
                        <Column>
                            <Label text="{i18n>freight}"/>
                        </Column>
                        <Column>
                            <Label text="{i18n>shipAddress}"/>
                        </Column>
                    </columns>
                    <items>
                        <ColumnListItem>    
                            <cells>
                                <ObjectIdentifier title="{jsonEmployees>OrderID}"/>
                                <Text text="{jsonEmployees>Freight}"/>
                                <Text text="{jsonEmployees>ShipAddress}"/>
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>

            </content>

            <endButton>
                <Button 
                        text="{i18n>close}"
                        press="onCloseOrders"/>
            </endButton>

        </Dialog>

</core:FragmentDefinition>